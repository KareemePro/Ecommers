@model Contact

<div class="card" bis_skin_checked="1">
    
    <div class="card-body" bis_skin_checked="1">
        <div class="card-title" bis_skin_checked="1">
            <h3 class="text-center title-2">Message</h3>
        </div>
        <hr>
        <form  novalidate="novalidate">
            <div class="form-group" bis_skin_checked="1">
                <label asp-for="Name"></label>
                <input asp-for="Name" class="form-control" />
                
            </div>
            <div class="form-group has-success" bis_skin_checked="1">
                <label asp-for="Email"></label>
                <input asp-for="Email" class="form-control" />
                
            </div>
            <div class="form-group" bis_skin_checked="1">
                <label asp-for="PhoneNumber"></label>
                <input asp-for="PhoneNumber" class="form-control"  />
                
            </div>
            
            <div class="form-group" bis_skin_checked="1">
                <label asp-for="Message"></label>
                <textarea asp-for="Message" rows="6" class="form-control" ></textarea>
                
            </div>
             <div>
                 <a asp-action="Index" class="btn btn-dark">Back</a>
             </div>  
           
        </form>
    </div>
</div>